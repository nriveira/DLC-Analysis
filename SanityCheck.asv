fs = 200;
ts = 1/fs;
t = ts:ts:10;
f1 = 10;
f2 = 60;

s1 = sin(2*pi*f1*t);
s2 = 50*sin(2*pi*f2*t);
sig = s1'+s2';

normSig = normalizeEEG(sig, fs);

result1 = get_wavelet_power(normSig, fs, [1 50], 20);
spec = mean(result1,2);

result1 = get_wavelet_power(normSig, fs, [1 50], 20);
spec = mean(result1,2);

result1 = get_wavelet_power(normSig, fs, [1 50], 20);
spec = mean(result1,2);

figure(1); hold on;
